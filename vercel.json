{
    "version": 2,
    "builds": [{
            "src": "frontEnd/**",
            "use": "@vercel/static-build",
            "config": { "distDir": "frontEnd/build" }
        },
        {
            "src": "backEnd/**",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "runtime": "@vercel/node"
        }
    }
}